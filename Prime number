#include"stdio.h"
void main()
{
    int num,flag = 0,i;
    printf("Enter num");
    scanf("%d",&num);
    for(i = 2;i<=num/2;i++){
         if(num%i==0){
            flag++;
            break;}
    }
     if(flag==0)
        printf("Prime number");
    else
        printf("Not a Prime number");
    
}
