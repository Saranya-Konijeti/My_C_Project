#include"stdio.h"
void main()
{
    float cel,far;
    printf("Enter celsius");
    scanf("%f",&cel);
    far = (cel * 9 / 5) + 32;
    printf("The converted celsius into farenheit is %f",far);
}


#include"stdio.h"
void main()
{
    float cel,far;
    printf("Enter farenheit");
    scanf("%f",&far);
    cel = (far - 32)*5/9; 
    
    printf("The converted celsius into farenheit is %f",cel);
}
